{
    "name": "fredbradley/imcapi",
    "description": "PHP JSON API wrapper for the HPE IMC software to manage network switches",
    "keywords": [
        "network monitoring",
        "network management",
        "intelligent management center",
        "hpe",
        "imc",
        "api"
    ],
    "homepage": "https://github.com/fredbradley/imcapi",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fred Bradley",
            "email": "code@fredbradley.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/http": "^7.0||^8.0||^9.0||^10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "vimeo/psalm": "^3.12"
    },
    "autoload": {
        "psr-4": {
            "FredBradley\\IMCAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FredBradley\\IMCAPI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "psalm": "vendor/bin/psalm --show-info=true",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    }
}
